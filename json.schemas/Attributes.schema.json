{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Attributes",
    "description": "An instance variable that can be applied to the model.",
    "type": "object",
    "minProperties": 1,
    "examples": [
        {"attributeName": {}}
    ],
    "patternProperties": {
        ".*": {
            "type": "object",
            "minProperties": 1,
            "maxProperties": 1,
            "oneOf": [
                {
                    "properties": {
                        "Bool": {
                            "$ref": "./datatypes/Bool.schema.json"
                        }
                    },
                    "required": ["Bool"]
                },
                {
                    "properties": {
                        "BrickColor": {
                            "$ref": "./datatypes/BrickColor.schema.json"
                        }
                    },
                    "required": ["BrickColor"]
                },
                {
                    "properties": {
                        "CFrame": {
                            "$ref": "./datatypes/CFrame.schema.json"
                        }
                    },
                    "required": ["CFrame"]
                },
                {
                    "properties": {
                        "Color3": {
                            "$ref": "./datatypes/Color3.schema.json"
                        }
                    },
                    "required": ["Color3"]
                },
                {
                    "properties": {
                        "ColorSequence": {
                            "$ref": "./datatypes/ColorSequence.schema.json"
                        }
                    },
                    "required": ["ColorSequence"]
                },
                {
                    "properties": {
                        "Float64": {
                            "$ref": "./datatypes/Float64.schema.json"
                        }
                    },
                    "required": ["Float64"]
                },
                {
                    "properties": {
                        "Font": {
                            "$ref": "./datatypes/Font.schema.json"
                        }
                    },
                    "required": ["Font"]
                },
                {
                    "properties": {
                        "NumberRange": {
                            "$ref": "./datatypes/NumberRange.schema.json"
                        }
                    },
                    "required": ["NumberRange"]
                },
                {
                    "properties": {
                        "NumberSequence": {
                            "$ref": "./datatypes/NumberSequence.schema.json"
                        }
                    },
                    "required": ["NumberSequence"]
                },
                {
                    "properties": {
                        "Rect": {
                            "$ref": "./datatypes/Rect.schema.json"
                        }
                    },
                    "required": ["Rect"]
                },
                {
                    "properties": {
                        "String": {
                            "$ref": "./datatypes/String.schema.json"
                        }
                    },
                    "required": ["String"]
                },
                {
                    "properties": {
                        "UDim": {
                            "$ref": "./datatypes/UDim.schema.json"
                        }
                    },
                    "required": ["UDim"]
                },
                {
                    "properties": {
                        "UDim2": {
                            "$ref": "./datatypes/UDim2.schema.json"
                        }
                    },
                    "required": ["UDim2"]
                },
                {
                    "properties": {
                        "Vector2": {
                            "$ref": "./datatypes/Vector2.schema.json"
                        }
                    },
                    "required": ["Vector2"]
                },
                {
                    "properties": {
                        "Vector3": {
                            "$ref": "./datatypes/Vector3.schema.json"
                        }
                    },
                    "required": ["Vector3"]
                }
            ]
        }
    }
}
