{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "description": "The properties of the instance",
    "properties": {
        "Axes": {
            "$ref": "./datatypes/Axes.schema.json"
        },
        "Attributes": {
            "$ref": "./Attributes.schema.json"
        },
        "BinaryString": {
            "$ref": "./datatypes/BinaryString.schema.json"
        },
        "Bool": {
            "$ref": "./datatypes/Bool.schema.json"
        },
        "BrickColor": {
            "$ref": "./datatypes/BrickColor.schema.json"
        },
        "CFrame": {
            "$ref": "./datatypes/CFrame.schema.json"
        },
        "Color3": {
            "$ref": "./datatypes/Color3.schema.json"
        },
        "Color3uint8": {
            "$ref": "./datatypes/Color3uint8.schema.json"
        },
        "ColorSequence": {
            "$ref": "./datatypes/ColorSequence.schema.json"
        },
        "Content": {
            "$ref": "./datatypes/Content.schema.json"
        },
        "Enum": {
            "$ref": "./datatypes/Enum.schema.json"
        },
        "Faces": {
            "$ref": "./datatypes/Faces.schema.json"
        },
        "Float32": {
            "$ref": "./datatypes/Float32.schema.json"
        },
        "Float64": {
            "$ref": "./datatypes/Float64.schema.json"
        },
        "Font": {
            "$ref": "./datatypes/Font.schema.json"
        },
        "Int32": {
            "$ref": "./datatypes/Int32.schema.json"
        },
        "Int64": {
            "$ref": "./datatypes/Int64.schema.json"
        },
        "MaterialColors": {
            "$ref": "./datatypes/MaterialColors.schema.json"
        },
        "NumberRange": {
            "$ref": "./datatypes/NumberRange.schema.json"
        },
        "NumberSequence": {
            "$ref": "./datatypes/NumberSequence.schema.json"
        },
        "OptionalCoordinateFrame": {
            "$ref": "./datatypes/OptionalCoordinateFrame.schema.json"
        },
        "PhysicalProperties": {
            "$ref": "./datatypes/PhysicalProperties.schema.json"
        },
        "ProtectedString": {
            "$ref": "./datatypes/ProtectedString.schema.json"
        },
        "Ray": {
            "$ref": "./datatypes/Ray.schema.json"
        },
        "Rect": {
            "$ref": "./datatypes/Rect.schema.json"
        },
        "Ref": {
            "$ref": "./datatypes/Ref.schema.json"
        },
        "Region3": {
            "$ref": "./datatypes/Region3.schema.json"
        },
        "Region3int16": {
            "$ref": "./datatypes/Region3int16.schema.json"
        },
        "SharedString": {
            "$ref": "./datatypes/SharedString.schema.json"
        },
        "String": {
            "$ref": "./datatypes/String.schema.json"
        },
        "Tags": {
            "$ref": "./datatypes/Tags.schema.json"
        },
        "UDim": {
            "$ref": "./datatypes/UDim.schema.json"
        },
        "UDim2": {
            "$ref": "./datatypes/UDim2.schema.json"
        },
        "Vector2": {
            "$ref": "./datatypes/Vector2.schema.json"
        },
        "Vector2int16": {
            "$ref": "./datatypes/Vector2int16.schema.json"
        },
        "Vector3": {
            "$ref": "./datatypes/Vector3.schema.json"
        },
        "Vector3int16": {
            "$ref": "./datatypes/Vector3int16.schema.json"
        }
    },
    "$defs": {
        "Axes": {
            "type": "array",
            "description": "Locks the object to move in a single axis.",
            "enum": [
                ["X","Y","Z"],
                ["X","Y"],
                ["Y","Z"],
                ["X","Z"],
                ["X"],
                ["Y"],
                ["Z"]
            ],
            "default": ["X","Y","Z"]
        },
        "Attributes": {
            "description": "An instance variable that can be applied to the model.",
            "type": "object",
            "minProperties": 1,
            "default": {"attributeName":{}},
            "patternProperties": {
                ".*": {
                    "type": "object",
                    "minProperties": 1,
                    "maxProperties": 1,
                    "oneOf": [
                        {
                            "properties": {
                                "Bool": {
                                    "$ref": "#/$defs/Bool"
                                }
                            },
                            "required": ["Bool"]
                        },
                        {
                            "properties": {
                                "BrickColor": {
                                    "$ref": "#/$defs/BrickColor"
                                }
                            },
                            "required": ["BrickColor"]
                        },
                        {
                            "properties": {
                                "CFrame": {
                                    "$ref": "#/$defs/CFrame"
                                }
                            },
                            "required": ["CFrame"]
                        },
                        {
                            "properties": {
                                "Color3": {
                                    "$ref": "#/$defs/Color3"
                                }
                            },
                            "required": ["Color3"]
                        },
                        {
                            "properties": {
                                "ColorSequence": {
                                    "$ref": "#/$defs/ColorSequence"
                                }
                            },
                            "required": ["ColorSequence"]
                        },
                        {
                            "properties": {
                                "Float64": {
                                    "$ref": "#/$defs/Float64"
                                }
                            },
                            "required": ["Float64"]
                        },
                        {
                            "properties": {
                                "Font": {
                                    "$ref": "#/$defs/Font"
                                }
                            },
                            "required": ["Font"]
                        },
                        {
                            "properties": {
                                "NumberRange": {
                                    "$ref": "#/$defs/NumberRange"
                                }
                            },
                            "required": ["NumberRange"]
                        },
                        {
                            "properties": {
                                "NumberSequence": {
                                    "$ref": "#/$defs/NumberSequence"
                                }
                            },
                            "required": ["NumberSequence"]
                        },
                        {
                            "properties": {
                                "Rect": {
                                    "$ref": "#/$defs/Rect"
                                }
                            },
                            "required": ["Rect"]
                        },
                        {
                            "properties": {
                                "String": {
                                    "$ref": "#/$defs/String"
                                }
                            },
                            "required": ["Rect"]
                        },
                        {
                            "properties": {
                                "UDim": {
                                    "$ref": "#/$defs/UDim"
                                }
                            },
                            "required": ["UDim"]
                        },
                        {
                            "properties": {
                                "UDim2": {
                                    "$ref": "#/$defs/UDim2"
                                }
                            },
                            "required": ["UDim2"]
                        },
                        {
                            "properties": {
                                "Vector2": {
                                    "$ref": "#/$defs/Vector2"
                                }
                            },
                            "required": ["Vector2"]
                        },
                        {
                            "properties": {
                                "Vector3": {
                                    "$ref": "#/$defs/Vector3"
                                }
                            },
                            "required": ["Vector3"]
                        }
                    ]
                }
            }
        },
        "BinaryString": {
            "type": "string",
            "description": "Base64 Encoded String",
            "default": ""
        },
        "Bool": {
            "type": "boolean",
            "description": "Boolean"
        },
        "BrickColor": {
            "type": "number",
            "description": "Default colors provided by Roblox.",
            "minimum": 1,
            "maximum": 1032,
            "default": 1
        },
        "CFrame": {
            "type": "object",
            "description": "Specifies the object's position and orientation in 3D space.",
            "properties": {
                "position": {
                    "type": "array",
                    "description": "[\n\tX,\n\tY,\n\tZ\n]",
                    "minItems": 3,
                    "maxItems": 3,
                    "items": {
                        "type": "number"
                    },
                    "default": [0.0,0.0,0.0]
                },
                "orientation": {
                    "type":"array",
                    "description": "[\n\t[X,Y,Z],\n\t[X,Y,Z],\n\t[X,Y,Z]\n]",
                    "minItems": 3,
                    "maxItems": 3,
                    "items": {
                        "type":"array",
                        "minItems":3,
                        "maxItems": 3,
                        "items": {
                            "type": "number"
                        }
                    },
                    "default": [[0,0,0],[0,0,0],[0,0,0]]
                }
            },
            "required": ["position", "orientation"]
        },
        "Color3": {
            "type": "array",
            "description": "Customizable colors. It uses normalized values for RGB.",
            "minItems": 3,
            "maxItems": 3,
            "items": {
                "type": "number",
                "minimum": 0,
                "maximum": 1
            },
            "default": [0.5,0.5,0.5]
        },
        "Color3uint8": {
            "type": "array",
            "description": "Customizable colors. It uses ranges of 0-255 for RGB.",
            "minItems": 3,
            "maxItems": 3,
            "items": {
                "type": "integer",
                "minimum": 0,
                "maximum": 255
            },
            "default": [126,126,126]
        },
        "ColorSequence": {
            "type": "object",
            "description": "Gradient of colors.",
            "properties": {
                "keypoints": {
                    "type": "array",
                    "description": "Specifies the stops for each color.",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "properties": {
                            "time": {
                                "type": "number",
                                "description": "Must be a normalized value.",
                                "minimum": 0,
                                "maximum": 1
                            },
                            "color": {
                                "$ref": "#/$defs/Color3"
                            }
                        },
                        "required": ["time", "color"]
                    }
                }
            },
            "required": ["keypoints"]
        },
        "Content": {
            "type": "string",
            "description": "A valid roblox assetid",
            "oneOf": [
                {
                    "pattern": "rbxassetid:\/{2}[0-9]+",
                    "examples": [
                        "rbxassetid://00000000000",
                        "rbxassetid://"
                        ]
                },
                {
                    "pattern": "rbxasset:\/{2}([A-Za-z0-9\\-\\_]+\/|[A-Za-z0-9\\-\\_]+\\.)*([A-Za-z0-9\\-\\_]+\\.[A-Za-z0-9]+)(?!.*\/+|.*\\.+)",
                    "examples": [
                        "rbxasset://folder-name/file_name.png",
                        "rbxasset://"
                    ],
                    "description": "Roblox Content Folder"
                },
                {
                    "pattern": "rbxthumb:\/{2}type=[A-Za-z]+&id=[0-9]+&w=[0-9]+&h=[0-9]+",
                    "examples": [
                        "rbxthumb://type=asset&id=00000000&w=00&h=00",
                        "rbxthumb://"
                        ],
                    "description": "Roblox Thumbnails"
                },
                {
                    "pattern": "rbxgameasset:\/{2}(|([A-Za-z0-9])+\/)+([A-Za-z0-9])+",
                    "examples": [
                        "rbxgameasset://folderName/fileName",
                        "rbxgameasset://"
                        ],
                    "description": "Asset Manager"
                },
                {
                    "pattern": "https?:\/{2}(w{3}|[A-Za-z]*)\\.roblox\\.com[A-Za-z\\?\/=0-9]+",
                    "examples": [
                        "https://create.roblox.com/",
                        "https://"
                        ],
                    "description": "Only supports Roblox-approved domains."
                },
                {
                    "pattern": "rbxtemp:\/{2}[0-9]+",
                    "examples": [
                        "rbxtemp://00",
                        "rbxtemp://"
                        ],
                    "description": "Files uploaded through StudioService:PromptImportFile."
                }
            ]
        },
        "Enum": {
            "type": "integer",
            "description": "The enum number",
            "minimum": 0
        },
        "Faces": {
            "type": "array",
            "description": "The sides of a model.",
            "items": {
                "enum": [
                    "Right", "Top", "Back", "Left", "Bottom", "Front"
                ]
            },
            "minItems": 1,
            "maxItems": 6,
            "uniqueItems": true
        },
        "Float32": {
            "type": "number",
            "description": "Float32"
        },
        "Float64": {
            "type": "number",
            "description": "Float64"
        },
        "Font": {
            "$comment": "TODO: Add enums",
            "type": "object",
            "description": "A Roblox-supported font",
            "properties": {
                "family": {
                    "type": "string"
                },
                "weight": {
                    "type": "string"
                },
                "style": {
                    "type": "string"
                }
            },
            "required": ["family", "weight", "style"]
        },
        "Int32": {
            "type": "integer",
            "description": "Int32"
        },
        "Int64": {
            "type": "integer",
            "description": "Int64"
        },
        "MaterialColors": {
            "type": "object",
            "description": "The texture and its color on Roblox meshes",
            "$comment": "TODO: Add materials enum",
            "properties": {
                "Grass": {
                    "$ref": "#/$defs/Color3uint8"
                }
            }
        },
        "NumberRange": {
            "type": "array",
            "description": "The minimum value to the maximum value allowed",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "type": "number"
            }
        },
        "NumberSequence": {
            "type": "object",
            "description": "A timeline of numbers changing until the final point in time",
            "properties": {
                "keypoints": {
                    "type": "array",
                    "description": "Specifies the stop of each number",
                    "minItems": 1,
                    "items": {
                        "properties": {
                            "time": {
                                "type": "number"
                            },
                            "value": {
                                "type":"number"
                            },
                            "envelope": {
                                "type": "number"
                            }
                        },
                        "required": ["time","value","envelope"]
                    }
                }
            },
            "required": ["keypoints"]
        },
        "OptionalCoordinateFrame": {
            "type": "null",
            "description": "WIP"
        },
        "PhysicalProperties": {
            "description": "Specifies the physics of the model",
            "oneOf": [
                {
                    "type": "string",
                    "enum": ["Default"],
                    "default": "Default"
                },
                {
                    "type": "object",
                    "properties": {
                        "density": {
                            "type": "number"
                        },
                        "friction": {
                            "type": "number"
                        },
                        "elasticity": {
                            "type": "number"
                        },
                        "frictionWeight": {
                            "type": "number"
                        },
                        "elasticityWeight": {
                            "type": "number"
                        }
                    },
                    "required": ["density","friction","elasticity","frictionWeight","elasticityWeight"]
                }
            ]
        },
        "ProtectedString": {
            "description": "A lua code formatted as a string",
            "type": "string"
        },
        "Ray": {
            "description": "A sort of 3D pointer similar to vectors on a 2D plane",
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "origin": {
                            "$ref": "#/$defs/Vector3"
                        },
                        "direction": {
                            "$ref": "#/$defs/Vector3"
                        }
                    },
                    "required": ["origin", "direction"]
                },
                {
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 2,
                    "items": {
                        "$ref": "#/$defs/Vector3"
                    }
                }
            ]
        },
        "Rect": {
            "type": "array",
            "description": "Represents a rectangle on a 2D plane",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "$ref": "#/$defs/Vector2"
            }
        },
        "Ref": {
            "type": "null",
            "description": "WIP"
        },
        "Region3": {
            "type": "null",
            "description": "WIP"
        },
        "Region3int16": {
            "type": "null",
            "description": "WIP"
        },
        "SharedString": {
            "type": "null",
            "description": "WIP"
        },
        "String": {
            "type": "string",
            "description": "String"
        },
        "Tags": {
            "type": "array",
            "description": "Applies unique identifiers to models",
            "minItems": 1,
            "items": {
                "type": "string"
            }
        },
        "UDim": {
            "type": "array",
            "description": "The scale of the 2D element and its offset",
            "minItems": 2,
            "maxItems": 2,
            "prefixItems": [
                {
                    "type": "number"
                },
                {
                    "type": "integer"
                }
            ]
        },
        "UDim2": {
            "type": "array",
            "description": "Specifies the X or width and the Y or height of a 2D element",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "$ref": "#/$defs/UDim"
            }
        },
        "Vector2": {
            "type": "array",
            "description": "Specifies an X and Y value",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "type": "number"
            }
        },
        "Vector2int16": {
            "type": "array",
            "description": "Specifies an X and Y value. It is a signed 16-bit integer.",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "type": "integer"
            }
        },
        "Vector3": {
            "type": "array",
            "description": "Specifies an X, Y, and Z value",
            "minItems": 3,
            "maxItems": 3,
            "items": {
                "type": "number"
            }
        },
        "Vector3int16": {
            "type": "array",
            "description": "Specifies an X, Y, and Z value. It is a signed 16-bit integer.",
            "minItems": 3,
            "maxItems": 3,
            "items": {
                "type": "integer"
            }
        }
    }
}
