{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Axes",
    "description": "Locks the object to move in a single axis.",
    "properties": {
        "Axes": {
            "type": "array",
            "uniqueItems": true,
            "minItems": 1,
            "maxItems": 3,
            "items": {
                "enum": ["X", "Y", "Z"]
            }
        }
    },
    "required": ["Axes"]
}
